{
  "author": "若愚[RUOYU]",
  "version": "1.0.0",
  "prefix": ["`", "--"],
  "CSet": [
    { "key": ["v", "version"], "info": "check out version" },
    { "key": ["h", "help"], "info": "get help" },
    { "key": ["e", "exit"], "info": "cmd pattern off" },
    { "key": ["q", "quit"], "info": "cmd pattern off" },
    { "key": ["rf", "refresh"], "info": "refresh page" },
    { "key": ["rl", "reload"], "info": "reload resource" },
    { "key": ["top"], "info": "go2 top" },
    { "key": ["btm", "bottom"], "info": "go2 bottom" },
    { "key": ["esc", "escape"], "info": "escape input(lose focus)" },
    { "key": ["c", "clear"], "info": "clear input content" },
    { "key": ["g", "game"], "info": "start game" }
  ]
}
